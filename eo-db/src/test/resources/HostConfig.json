{
  "localMySql:core": {
    "configModelKey": "DbConfig",
    "properties": {
      "dbType": "MYSQL",
      "schema": "core"
    },
    "module": "eo-db",
    "moduleScope": "main",
    "description": "Core Schema"
  },
  "localMySql:test": {
    "configModelKey": "DbConfig",
    "properties": {
      "dbType": "MYSQL",
      "schema": "test"
    },
    "module": "eo-db",
    "moduleScope": "main",
    "description": "Test Schema"
  },
  "localOracle:XE": {
    "configModelKey": "DbConfig",
    "properties": {
      "schema": "XE",
      "dbType": "ORACLE"
    },
    "module": "eo-db",
    "moduleScope": "main",
    "description": "Oracle Schem"
  },
  "hsql:mem": {
    "configModelKey": "DbConfig",
    "properties": {
      "schema": "mem",
      "dbType": "HSQL"
    },
    "module": "eo-db",
    "moduleScope": "main",
    "description": "local HSQL which exists just in memory"
  },
  "hsql:fileTest": {
    "configModelKey": "DbConfig",
    "properties": {
      "schema": "file:$[TMP]/test.hsql",
      "dbType": "HSQL"
    },
    "module": "eo-db",
    "moduleScope": "main",
    "description": "local HSQL with a temp @see http://hsqldb.org/doc/guide/dbproperties-chapt.html"
  },
  "h2:mem": {
    "configModelKey": "DbConfig",

    "properties": {
      "protocol": "jdbc:h2",
      "user": "sa",
      "schema": "mem:test",
      "driver": "org.h2.Driver",
      "dbType": "H2",
      "extension": "DATABASE_TO_UPPER=false;"
    },
    "module": "eo-db",
    "moduleScope": "main",
    "description": "local H2 which exists just in memory"
  },
  "h2:file": {
    "module": "eo-db",
    "moduleScope": "main",
    "configModelKey": "DbConfig",
    "properties": {
      "protocol": "jdbc:h2",
      "user": "sa",
      "schema": ".{TMP}./test",
      "driver": "org.h2.Driver",
      "dbType": "H2",
      "extension": "DATABASE_TO_UPPER=false;"
    },
    "description": "local H2 with a temp file"
  }
}